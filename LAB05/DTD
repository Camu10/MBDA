<?xml version="1.0"?>
<!DOCTYPE Course_Catalog[
	<!ELEMENT Course_Catalog(Department)
		<!ELEMENT Department(Title,Chair,Course+)>
			<!ELEMENT Title (#PCDATA)>
			<!ELEMENT Chair(Professor+)>
                <!ELEMENT Professor (First_Name, Last_Name)>
                    <!ELEMENT First_Name (#PCDATA)>
                    <!ELEMENT Last_Name (#PCDATA)>
			<!ELEMENT Course (Title*, Description*, Instructors*,Prerequisites*)>
				<!ELEMENT Title (#PCDATA)>
				<!ELEMENT Description (#PCDATA)>
				<!ELEMENT Instructors(Lecturer+,Professor*)>
					<!ELEMENT Lecturer(First_Name, Last_Name)>
                        <!ELEMENT First_Name (#PCDATA)>
                        <!ELEMENT Middle_Initial (#PCDATA)>
                        <!ELEMENT Last_Name (#PCDATA)>
                    <!ELEMENT Professor (First_Name, Last_Name)>
                        <!ELEMENT First_Name (#PCDATA)>
                        <!ELEMENT Last_Name (#PCDATA)>
                <!ELEMENT Prerequisites(Prereq*)>
                    <!ELEMENT Prerequisites(#PCDATA)>
            <!ATTLIST Department
                Code CDATA #REQUIRED>
            <!ATTLIST Course
                Number CDATA #REQUIRED
                Enrollment CDATA #REQUIRED>
]>
-- XML Nook
-- XML NoOK: Hay más de un nodo raíz (Course_Catalog,ola_k_ase)
<Course_Catalog>
    <Departament Code = "SE">
        <Title> System Engineering </Title>
        <Chair>
            <Professor>
                <First_Name>Oswaldo</First_Name>
                <Last_Name>Castillo</Last_Name>
            </Professor>
        </Chair>
        <Course Number="CS110" Enrollment="2000">
            <Title>Modelos Matematicos</Title>
            <Description>
                Introduccion a la logica matematica.
            </Description>
            <Instructors>
